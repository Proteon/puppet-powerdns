# PostgreSQL Configuration
#
# Managed by Puppet
#
# Launch gpgsql backend
launch=gpgsql

# gpgsql parameters
<% if @socket %>
    gpgsql-socket=<%=@socket%>
<% else %>
    gpgsql-host=<%= @host %>
    gpgsql-port=<%= @port %>
<% end -%>
gpgsql-dbname=<%= @dbname %>
gpgsql-user=<%= @user %>
gpgsql-password=<%= @password %>
<% if @dnssec%>
gpgsql-dnssec
<%end-%> 
